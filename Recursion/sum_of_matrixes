#include<bits/stdc++.h>
using namespace std;
int sum(vector<vector<int>>&a,vector<vector<int>>&b,int i,int j,vector<vector<int>>&r)
{
    if(i==a.size()){
        return 0;
    }
    if(j==a[0].size()){
        return sum(a,b,i+1,0,r);
    }
    r[i][j] = a[i][j] + b[i][j];
    return sum(a,b,i,j+1,r);
}
int main(){
    int r,c;
    cin>>r>>c;
    vector<vector<int>>a(r,vector<int>(c));
    vector<vector<int>>b(r, vector<int>(c));
    vector<vector<int>>res(r, vector<int>(c));
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            cin>>a[i][j];
        }
    }
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++) {
            cin>>b[i][j];
        }
    }

    sum(a,b,0,0,res);
    for(int i= 0;i<r;i++){
        for(int j=0;j<c;j++){
            cout<<res[i][j]<<" ";
        }
        cout<<endl;
    }
    return 0;
}
